{
  "name": "apollo-level2-assignment-2",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "start:dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint src --fix",
    "prettier": "prettier src --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prettier:format": "npx prettier --write src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.4.0",
    "zod": "^3.23.8"
  }
}
